{
    // (Code Runner) ターミナルで実行する
    "code-runner.runInTerminal": true,
    // (Code Runner) 言語毎の実行プログラムのパス指定
    "code-runner.executorMap": {
        "java": "${JAVA_HOME}/bin/javac $fullFileName -d \"$workspaceRoot/bin\" && ${JAVA_HOME}/bin/java -cp \"$workspaceRoot/bin\" $fileNameWithoutExt",
        "perl": "/usr/bin/perl",
        "python": "/usr/local/bin/python3 -u",
        "ruby": "/usr/bin/ruby",
    }
}